<configuration>
    <system.web>
      <compilation debug="true" targetFramework="4.5" />
      <httpRuntime targetFramework="4.5" />
      <webServices>
        <protocols>
          <add name="HttpPost"></add>
          <add name="HttpGet"></add>
          <add name="HttpSoap"></add>
          <add name="Documentation"></add>
        </protocols>
      </webServices>
    </system.web>
</configuration>
