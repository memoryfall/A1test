        })
        var slider = appcan.slider({
            selector : '#slider',
            aspectRatio : 8 / 16,
            hasLabel : true,
            index : 0
        });
        slider.set([{
            img : "index_content/css/myImg/main.png",
            label : "主页"
        }, {
            img : "",
            label : "111"
        }, {
